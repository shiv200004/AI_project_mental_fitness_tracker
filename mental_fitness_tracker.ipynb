mean= dataset['Mental_disorder'].mean()
mean

fig = px.pie(dataset,values='Mental_disorder',names='Year')
fig.show()

fig = px.bar(dataset.head(10),x='Year',y='Mental_disorder',color='Year',template='ggplot2')
